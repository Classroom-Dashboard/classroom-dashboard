<!-- Fields prompted to the user to create a new Section -->

<%= simple_form_for @section do |f| %>
	<%= f.input :course, label: "Course Name" %>
	<%= f.input :semester, label: "Semester (Fall? Spring? Summer?)" %>
	<%= f.input :year %>
	<%= f.input :professor, label: "Professor(s)" %>
	<%= f.button :submit %>
<% end %>